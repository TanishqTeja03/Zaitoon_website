    <!--Confirmed Table Section  -->
        <h3 class="table_title">Booking Details</h3><br><br>
        <table data-aos="fade-up">
          <thead>
            <tr>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Email</th>
              <th>Table Type</th>
              <th>Guest Number</th>
              <th>Placement</th>
              <th>Date</th>
              <th>Time</th>
              <th>Note</th>
            </tr>
          </thead>
          <tbody>
            <% confirmed.forEach(function(booking) { %>
            <tr>
              <td><%= booking.firstName %></td>
              <td><%= booking.lastName %></td>
              <td><%= booking.email %></td>
              <td><%= booking.tableType %></td>
              <td><%= booking.guestNumber %></td>
              <td><%= booking.placement %></td>
              <td><%= booking.date %></td>
              <td><%= booking.time %></td>
              <td><%= booking.note %></td>
            </tr>
            <% }); %>
          </tbody>
        </table><br><br>
